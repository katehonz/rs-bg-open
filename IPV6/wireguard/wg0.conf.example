# WireGuard конфигурация от ipv6.rs
# Копирай този файл като wg0.conf и попълни с данните от ipv6.rs

[Interface]
PrivateKey = YOUR_PRIVATE_KEY_HERE
Address = YOUR_IPV6_ADDRESS/64
# Например: 2001:db8:1234:5678::1/64

# DNS сървъри (опционално)
DNS = 2001:4860:4860::8888, 2001:4860:4860::8844

[Peer]
PublicKey = SERVER_PUBLIC_KEY_HERE
Endpoint = SERVER_ENDPOINT:51820
# Например: vpn.ipv6.rs:51820

# Разреши целия IPv6 трафик през tunnel
AllowedIPs = ::/0

# Keep alive пакети на всеки 25 секунди
PersistentKeepalive = 25
